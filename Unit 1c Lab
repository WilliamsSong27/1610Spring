using System;

public class Program
{
    public static void Main()
    {
        int[] numbers = {1, 2, 3, 4, 5};
        int rows = numbers.Length; // Number of rows for the pyramid

        // Outer loop for rows
        for (int i = 0; i < rows; i++)
        {
            // Inner loop for printing the numbers in the pyramid
            for (int j = 0; j <= i; j++)
            {
                Console.Write(numbers[i]); // Print the current row number i
            }
            Console.WriteLine(); // Move to the next line after each row
        }
    }
}

